<!DOCTYPE html>
<html>
	<head>
		<title>Usuarios</title>
		<% include ./head %>
	</head>
	<body>
		<header>
		<% include ./header %>
		</header>
		<div class="row">
			<div class="col s8 offset-s2">
				<main>
					<h1>Usuarios</h1>
					<table>
						<thead>
							<tr>
								<th>Nombre de Invocador</th>
								<th>Nivel</th>
								<th>Liga/Divisi√≥n</th>
							</tr>
						</thead>
						<tbody>
							<% users.forEach(function(user) { %>
							<tr>
								<td><%= user.name %></td>
								<td><%= user.level %></td>
								<td><%= user.rank %></td>
							</tr>
							<% }); %>
						</tbody>
					</table>
				</main>
			</div>
		</div>
		<% include ./footer %>
	</body>
</html>
